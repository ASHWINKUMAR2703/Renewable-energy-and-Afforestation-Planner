                    <div class="card-body">
                        
                           <div class="form-group row">
                                <label for="password" class="col-md-4 col-form-label text-md-right">Number of plants </label>
                                <div class="col-md-6">
                                    <input @bind-value=@nop id="openspacearea" class="form-control" name="openspacearea" type="text"  required autofocus>    
                                </div><h5>m2</h5>   
                            </div>  
                            <div class="form-group row">
                                <label for="password" class="col-md-4 col-form-label text-md-right">Area required for Miyawakki forestry</label>
                                <div class="col-md-6">
                                    <input @bind-value=@arfmf id="openspacearea" class="form-control" name="openspacearea" type="text"  required autofocus>    
                                </div><h5>m2</h5>   
                            </div>  
                            <div class="form-group row">
                                <label for="password" class="col-md-4 col-form-label text-md-right">Area required for Conventional forestry</label>
