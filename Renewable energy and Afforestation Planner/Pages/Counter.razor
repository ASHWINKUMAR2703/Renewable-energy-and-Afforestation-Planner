                    </div>
                    </div>



                </div>



            </div>
        </div>
    </div>
   
</main>
@code {
    string srtp; string cosrtc; string sciip; string sdtoip;string srtp2; string cosrtc2;string sciip2; string sdtoip2;string srtp3;string cosrtc3;string sciip3;string sdtoip3; string acr;string cooss; string sciip4;string sdomsp;
    string nop; string arfmf;string afcf; string cop;
    protected override void OnInitialized()
    {
         srtp =Convert.ToString( Pages.FetchData.SolarRooftoppotential);
         cosrtc = Convert.ToString( Pages.FetchData.CostofSolarRooftopCAPEX);
         sciip = Convert.ToString( Pages.FetchData.SavingsduetoCAPEXinvsetmentinsolarplant);
         sdtoip = Convert.ToString( Pages.FetchData.SavingsduetoOPEXinsolarplant);

         srtp2 = Convert.ToString( Pages.FetchData.SolarRooftoppotential2);
         cosrtc2 = Convert.ToString( Pages.FetchData.CostofSolarRooftopCAPEX2);
         sciip2 = Convert.ToString( Pages.FetchData.SavingsduetoCAPEXinvsetmentinsolarplant2);
         sdtoip2 = Convert.ToString( Pages.FetchData.SavingsduetoOPEXinsolarplant2);

         srtp3 = Convert.ToString( Pages.FetchData.SolarRooftoppotential3);
         cosrtc3 = Convert.ToString( Pages.FetchData.CostofSolarRooftopCAPEX3);
         sciip3 = Convert.ToString( Pages.FetchData.SavingsduetoCAPEXinvsetmentinsolarplant3);
         sdtoip3 = Convert.ToString( Pages.FetchData.SavingsduetoOPEXinsolarplant3);

         acr = Convert.ToString( Pages.FetchData.AdditionalCapacityrequired);
         cooss = Convert.ToString( Pages.FetchData.CostofoffsiteSolarsystem);
         sciip4 = Convert.ToString( Pages.FetchData.SavingsduetoCAPEXinvsetmentinsolarplant4);
         sdomsp = Convert.ToString( Pages.FetchData.SavingsduetoOPEXmodelsolarplant);

         nop =Convert.ToString( Pages.FetchData.Numberofplants);
         arfmf =Convert.ToString( Pages.FetchData.ArearequiredforMiyawakkiforestry);
         afcf =Convert.ToString( Pages.FetchData.ArearequiredforConventionalforestry);
         cop =Convert.ToString( Pages.FetchData.Costofplantation);
    }

}
