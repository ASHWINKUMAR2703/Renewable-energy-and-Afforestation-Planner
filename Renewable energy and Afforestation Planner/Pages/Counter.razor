@page "/counter"

<main class="login-form">
   
    <div class="cotainer">
        <div class="row justify-content-center">
            <div class="col-md-8">
                <div class="card" >
                    <div class="card-header"style="background-color: #2e2e38"> <a style=" font-family: Georgia, serif; font-size: 20px; color: white; text-align:center">Solar Rooftop</a></div>
                    <div class="card-body">
                        
                            <div class="form-group row">
                                <label for="email_address" class="col-md-4 col-form-label text-md-right">Solar Rooftop Potential</label>
                                <div class="col-md-6">
                                    <input @bind-value=@srtp id="rooftoparea" class="form-control" name="rooftoparea"  type="text" required autofocus>                                     
                                </div><h5>KWh</h5>   
                            </div>

                            <div class="form-group row">
                                <label for="password" class="col-md-4 col-form-label text-md-right">Cost of Solar Rooftop CAPEX</label>
                                <div class="col-md-6">
                                    <input @bind-value=@cosrtc id="openspacearea" class="form-control" name="openspacearea" type="text"  required autofocus>    
                                </div><h5>Rs</h5>   
                            </div>

                               <div class="form-group row">
                                <label for="password" class="col-md-4 col-form-label text-md-right">Savings due to CAPEX invsetment in solar plant</label>
                                <div class="col-md-6">
                                    <input @bind-value=@sciip id="openspacearea" class="form-control" name="openspacearea" type="text"  required autofocus>    
                                </div><h5>Rs/annum</h5>   
                            </div>

                               <div class="form-group row">
                                <label for="password" class="col-md-4 col-form-label text-md-right">Savings due to OPEX in solar plant</label>
                                <div class="col-md-6">
                                    <input @bind-value=@sdtoip id="openspacearea" class="form-control" name="openspacearea" type="text"  required autofocus>    
                                </div><h5>Rs/annum</h5>   
                            </div>

                           
                    </div>
                   <div class="card" >
                    <div class="card-header"style="background-color: #2e2e38"> <a style=" font-family: Georgia, serif; font-size: 20px; color: white; text-align:center">Solar installation in open area</a></div>
                    <div class="card-body">
                        
                           <div class="form-group row">
                                <label for="email_address" class="col-md-4 col-form-label text-md-right">Solar Rooftop Potential</label>
                                <div class="col-md-6">
                                    <input @bind-value=@srtp2 id="rooftoparea" class="form-control" name="rooftoparea"  type="text" required autofocus>                                     
                                </div><h5>KWh</h5>   
